{
  "name": "obfetch",
  "version": "0.0.3",
  "description": "An http client based on rxjs and fetch API inspired by Angular HttpClient.",
  "main": "./dist/index.js",
  "type": "module",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "coverage": "vitest run --coverage",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wflixu/obfetch.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "keywords": [
    "rxjs",
    "fetch",
    "observable",
    "ts",
    "xhr",
    "http",
    "HttpClient",
    "retry"
  ],
  "author": "wflixu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wflixu/obfetch/issues"
  },
  "homepage": "https://github.com/wflixu/obfetch#readme",
  "devDependencies": {
    "@types/node": "^20.9.4",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.34.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "typescript-language-server": "^3.3.0",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "rxjs": "^7.4.0"
  },
  "dependencies": {
    "rxjs": "^7.4.0"
  }
}
