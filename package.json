{
  "name": "obfetch",
  "version": "1.0.0",
  "description": "A http client based on rxjs and fetch API inspired by Angular HttpClient.",
  "main": "./dist/bundle.js",
  "type": "module",
  "module": "dist/bundle.js",
  "unpkg": "dist/bundle.umd.js",
  "types": "typings/index.d.ts",
  "scripts": {
    "build": "rollup --config rollup.config.mjs",
    "tc": "tsc",
    "cl": "rm -rf ./dist && rm -rf ./typings",
    "coverage": "vitest run --coverage",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wflixu/obfetch.git"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/bundle.mjs",
      "require": "./dist/bundle.cjs",
      "default": "./dist/bundle.js",
      "types": "./typings/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "src/",
    "typings/",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "keywords": [
    "rxjs",
    "fetch",
    "observable",
    "ts",
    "xhr",
    "http",
    "HttpClient",
    "retry"
  ],
  "author": "wflixu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wflixu/obfetch/issues"
  },
  "homepage": "https://github.com/wflixu/obfetch#readme",
  "devDependencies": {
    "@biomejs/biome": "1.4.1",
    "@rollup/plugin-json": "^6.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.9.4",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "@vitest/coverage-v8": "^0.34.6",
    "eslint": "^8.34.0",
    "fastify": "^4.24.3",
    "rollup": "^4.6.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2",
    "typescript-language-server": "^3.3.0",
    "vitest": "^0.34.6"
  },
  "peerDependencies": {
    "rxjs": "^7.4.0"
  },
  "dependencies": {
    "rxjs": "^7.4.0"
  }
}
